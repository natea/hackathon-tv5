{
  "swarmConfiguration": {
    "topology": "hierarchical",
    "maxAgents": 10,
    "strategy": "adaptive",
    "coordination": {
      "mode": "distributed",
      "consensus": "raft",
      "failover": "automatic"
    }
  },
  "agentRoles": {
    "queen": {
      "type": "coordinator",
      "responsibilities": ["strategic-planning", "resource-allocation", "conflict-resolution"],
      "priority": "critical"
    },
    "specialists": [
      {
        "type": "researcher",
        "domain": "iMessage-analysis",
        "capabilities": ["data-extraction", "pattern-recognition", "sentiment-analysis"]
      },
      {
        "type": "coder",
        "domain": "implementation",
        "capabilities": ["feature-development", "refactoring", "optimization"]
      },
      {
        "type": "tester",
        "domain": "quality-assurance",
        "capabilities": ["unit-testing", "integration-testing", "test-coverage"]
      },
      {
        "type": "reviewer",
        "domain": "code-quality",
        "capabilities": ["code-review", "security-audit", "best-practices"]
      }
    ],
    "workers": [
      {
        "type": "analyst",
        "capabilities": ["performance-analysis", "bottleneck-detection", "metrics-collection"]
      },
      {
        "type": "documenter",
        "capabilities": ["api-documentation", "user-guides", "technical-specs"]
      }
    ]
  },
  "taskDistribution": {
    "loadBalancing": "adaptive",
    "priorityQueue": true,
    "parallelExecution": true,
    "batchProcessing": {
      "enabled": true,
      "maxBatchSize": 5
    }
  },
  "monitoring": {
    "realTime": true,
    "metrics": ["task-completion", "agent-utilization", "response-time", "error-rate"],
    "alerts": {
      "enabled": true,
      "thresholds": {
        "errorRate": 0.05,
        "responseTime": 5000,
        "agentUtilization": 0.9
      }
    }
  },
  "optimization": {
    "autoScaling": true,
    "resourceOptimization": true,
    "caching": {
      "enabled": true,
      "ttl": 3600
    },
    "neural": {
      "patternLearning": true,
      "adaptiveStrategies": true
    }
  },
  "integrations": {
    "github": {
      "enabled": true,
      "features": ["pr-review", "issue-triage", "code-analysis"]
    },
    "memory": {
      "persistent": true,
      "crossSession": true,
      "namespace": "conversational-reflection"
    }
  }
}
