    "tsx": "^4.15.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@photon-ai/imessage-kit": "^0.1.0",
